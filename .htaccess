RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}.php -f
RewriteRule ^([^\.]+)/?$ $1.php [L]

<FilesMatch "\.json$">
    Order allow,deny
    Deny from all
</FilesMatch>

php_value max_execution_time 1800
php_value max_input_time 1800
php_value memory_limit 512M
php_value max_input_vars 10000

